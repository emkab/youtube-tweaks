{
	"name": "em@kab's YouTube Playlist Search",
	"description": "em@kab's YouTube playlist search tool designed to be fast, intuitive, efficient, and easy on the eyes.",
	"version": "1.0.0.2",
	"manifest_version": 3,
	"action": {},
	"content_scripts": [
		{
			"js": ["tweaks/playlist_search/tweak.js"],
			"matches": ["https://www.youtube.com/playlist?*"],
			"run_at": "document_end"
		}
	],
	"icons": {
		"32": "icons/icon32.png",
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	}
}
